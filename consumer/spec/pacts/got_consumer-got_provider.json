{
  "consumer": {
    "name": "Got Consumer"
  },
  "provider": {
    "name": "Got Provider"
  },
  "interactions": [
    {
      "description": "a request to get the characters",
      "providerState": "a list of characters exists",
      "request": {
        "method": "get",
        "path": "/characters",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "characters": [
            {
              "name": "Arya Stark",
              "age": 18,
              "house": "Stark",
              "actor": "Maisie Williams"
            }
          ]
        },
        "matchingRules": {
          "$.headers.Content-Type": {
            "match": "regex",
            "regex": "^application\\/json(;\\s*charset=[\\w-]+)?$"
          },
          "$.body.characters": {
            "min": 1
          },
          "$.body.characters[*].*": {
            "match": "type"
          },
          "$.body.characters[*].name": {
            "match": "type"
          },
          "$.body.characters[*].age": {
            "match": "type"
          },
          "$.body.characters[*].house": {
            "match": "type"
          },
          "$.body.characters[*].actor": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}